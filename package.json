{
  "name": "nagdl",
  "version": "1.0.7",
  "description": "Modified play-dl for my bot",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "tsup",
    "build:check": "tsc --noEmit --incremental false",
    "pretty": "prettier --config .prettierrc \"nagdl/*.ts\" \"nagdl/*/*.ts\" \"nagdl/*/*/*.ts\" --write ",
    "prepublishOnly": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3Gigs/nagdl"
  },
  "keywords": [
    "nagdl",
    "nagdb"
  ],
  "author": "3Gigs",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/3Gigs/nagdl/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/3Gigs/nagdl#readme",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/node": "^16.9.4",
    "prettier": "^2.3.1",
    "tsup": "^5.11.1",
    "typedoc": "^0.22.9",
    "typedoc-plugin-extras": "^2.2.1",
    "typedoc-plugin-missing-exports": "^0.22.4",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "play-audio": "^0.4.3"
  }
}
